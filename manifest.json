{
	"manifest_version": 2,
	"name": "Relaxing Kitties TM",
	"description": "Расслабляющие котики в твоем браузере!",
	"version": "1.0.0",
	"icons": {"128": "icon.png"},
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "index.html"
	},
	"permissions": [
		"activeTab",
		"*://*/*"
	],
	"content_security_policy": "script-src 'self' 'unsafe-eval' https://fonts.googleapis.com; object-src 'self'"
}